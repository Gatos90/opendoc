{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "opendoc",
  "description": "AI-powered development tool",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "scripts": {
    "dev": "bun run --cwd packages/opendoc src/index.ts",
    "typecheck": "bun turbo typecheck",
    "test": "echo 'do not run tests from root' && exit 1"
  },
  "workspaces": {
    "packages": [
      "packages/opendoc",
      "packages/sdk/js",
      "packages/plugin",
      "packages/util",
      "packages/script"
    ],
    "catalog": {
      "@types/bun": "1.3.5",
      "@types/node": "22.13.9",
      "@tsconfig/bun": "1.0.9",
      "@tsconfig/node22": "22.0.2",
      "@hono/zod-validator": "0.4.2",
      "@octokit/rest": "22.0.0",
      "@openauthjs/openauth": "0.0.0-20250322224806",
      "@typescript/native-preview": "7.0.0-dev.20251207.1",
      "ulid": "3.0.1",
      "@pierre/diffs": "1.0.2",
      "diff": "8.0.2",
      "ai": "5.0.119",
      "hono": "4.10.7",
      "hono-openapi": "1.1.2",
      "fuzzysort": "3.1.0",
      "typescript": "5.8.2",
      "zod": "4.1.8",
      "remeda": "2.26.0",
      "shiki": "3.20.0"
    }
  },
  "devDependencies": {
    "@tsconfig/bun": "catalog:",
    "prettier": "3.6.2",
    "turbo": "2.5.6"
  },
  "dependencies": {
    "@opendoc-ai/plugin": "workspace:*",
    "@opendoc-ai/script": "workspace:*",
    "@opendoc-ai/sdk": "workspace:*",
    "typescript": "catalog:"
  },
  "repository": {
    "type": "git",
    "url": "https://opendoc.creativdev.org"
  },
  "license": "MIT",
  "prettier": {
    "semi": false,
    "printWidth": 120
  },
  "trustedDependencies": [
    "esbuild",
    "protobufjs",
    "tree-sitter",
    "tree-sitter-bash",
    "web-tree-sitter"
  ],
  "overrides": {
    "@types/bun": "catalog:",
    "@types/node": "catalog:"
  }
}
